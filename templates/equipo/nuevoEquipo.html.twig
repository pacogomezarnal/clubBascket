{% extends 'base.html.twig' %}
{% block body %}
    <div class="form_small">
    {{ form_start(form) }}
        <div>
                {{ form_errors(form) }}
        </div>
        <div class="form-group">
            {{ form_label(form.categoria) }}
            {{ form_widget(form.categoria, {'attr': {'class': 'form-control'}}) }}
            {{ form_help(form.categoria, {'attr': {'class': 'form-text text-muted'}}) }}
        </div>
        <div class="form-group">
            {{ form_label(form.sexo) }}
            {{ form_widget(form.sexo, {'attr': {'class': 'form-control'}}) }}
            {{ form_help(form.sexo, {'attr': {'class': 'form-text text-muted'}}) }}
        </div>
        <div class="form-group">
            {{ form_label(form.numjugadores) }}
            {{ form_widget(form.numjugadores, {'attr': {'class': 'form-control'}}) }}
            {{ form_help(form.numjugadores, {'attr': {'class': 'form-text text-muted'}}) }}
        </div>
        {{ form_label(form.save) }}
    {{ form_end(form) }}
    </div>
{% endblock %}